{
  "name": "fin/user-backup-lib",
  "description": "Библиотека PHP для резервного копирования пользовательских данных из нескольких баз данных.",
  "type": "library",
  "require": {
    "php": "^8.0",
    "illuminate/support": "^8.0",
    "illuminate/database": "^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "App\\UserBackupServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://repo.packagist.org"
    }
  },
  "config": {
    "secure-http": true,
    "platform": {
      "php": "8.0.30"
    }
  }
}
