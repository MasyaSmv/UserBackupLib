{
  "name": "fin/user-backup-lib",
  "description": "Библиотека PHP для резервного копирования пользовательских данных из нескольких баз данных.",
  "type": "library",
  "require": {
    "php": "^7.4",
    "illuminate/support": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "App\\Services\\": "src/Services/",
      "App\\Exceptions\\": "src/Exceptions/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "App\\Services\\UserBackupService"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
